<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChooseBar">

    <ImageView
        android:id="@+id/shortysBar"
        android:layout_width="210dp"
        android:layout_height="185dp"
        android:src="@drawable/shortys"
        app:layout_constraintBottom_toTopOf="@+id/ktownPub"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <ImageView
        android:id="@+id/ktownPub"
        android:layout_width="231dp"
        android:layout_height="156dp"
        android:src="@drawable/ktownpub"
        app:layout_constraintBottom_toTopOf="@+id/kutzTavern"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/shortysBar" />

    <ImageView
        android:id="@+id/kutzTavern"
        android:layout_width="247dp"
        android:layout_height="110dp"
        android:src="@drawable/ktavern"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ktownPub" />

    <TextView
        android:id="@+id/barsHeader"
        android:layout_width="192dp"
        android:layout_height="47dp"
        android:text="Local Bars:"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="191dp"
        android:layout_height="41dp"
        android:text="Kutzown, PA"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/shortysBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barsHeader" />
</androidx.constraintlayout.widget.ConstraintLayout>